    "@babel/preset-env": "^7.14.0",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^26.0.23",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "jest-junit": "^12.1.0",
    "rollup": "^2.47.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
